{
  "name": "react-component-viewer",
  "version": "0.18.0",
  "description": "React Component to help with development of other React components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MykolaGolubyev/react-component-viewer.git"
  },
  "author": "mykolagolubyev",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MykolaGolubyev/react-component-viewer/issues"
  },
  "homepage": "https://github.com/MykolaGolubyev/react-component-viewer#readme",
  "private": false,
  "main": "build/lib/components/index.js",
  "types": "build/lib/components/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "build/lib"
  ],
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "url-search-params-polyfill": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "tsc && npm run copy-css-to-lib",
    "test": "react-scripts-ts test --env=jsdom",
    "copy-css-to-lib": "npm-run-all -s copy-base-css copy-component-css",
    "copy-base-css": "cpx \"./src/**/*.css\" ./build/lib/",
    "copy-component-css": "cpx \"./src/components/**/*.css\" ./build/lib/components"
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^9.4.0",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "cpx": "^1.5.0",
    "jest": "^22.4.3",
    "jest-cli": "^24.7.1",
    "npm-run-all": "^4.1.2",
    "react-scripts-ts": "2.13.0",
    "typescript": "3.4.3",
    "webpack": "^3.10.0"
  }
}
